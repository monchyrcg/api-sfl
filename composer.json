{
  "name": "monchyrcg/api-sfl",
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0",

    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^5.3",
    "symfony/messenger": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/twig-bundle": "^5.3",
    "symfony/validator": "^5.3",

    "lambdish/phunctional": "^2.0",

    "ramsey/uuid": "^3.9",

    "doctrine/dbal": "^2.10",
    "doctrine/orm": "^2.7",
    "monolog/monolog": "^2.1",
    "symfony/runtime": "^5.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",

    "behat/behat": "^3.6",
    "friends-of-behat/mink-extension": "^2.4",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.1",

    "phpunit/phpunit": "^9.2",
    "mockery/mockery": "^1.4",

    "symfony/debug": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/src",
      "Sfl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/src",
      "Sfl\\": "src",
      "Sfl\\Tests\\": ["tests"]
    }
  },
  "extra":{
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    },
    "config-dir": "app/config",
    "bin-dir": "app/bin",
    "src-dir": "app/src",
    "public-dir": "app/public"
  },
  "minimum-stability": "RC"
}
