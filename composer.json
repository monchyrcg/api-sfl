{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/orm": "^2.9",
    "lambdish/phunctional": "^2.1",
    "nesbot/carbon": "^2.53",
    "ramsey/uuid": "4.2",
    "symfony/console": "5.3.*",
    "symfony/dotenv": "5.3.*",
    "symfony/framework-bundle": "5.3.*",
    "symfony/messenger": "5.3.*",
    "symfony/proxy-manager-bridge": "5.3.*",
    "symfony/runtime": "5.3.*",
    "symfony/string": "5.3.*",
    "symfony/yaml": "5.3.*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Sfl\\": "src/",
      "App\\": "app/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sfl\\": "src/",
      "App\\": "app/src/",
      "App\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    },
    "config-dir": "app/config",
    "bin-dir": "app/bin",
    "src-dir": "app/src",
    "public-dir": "app/public"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "friends-of-behat/symfony-extension": "^2.1",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "5.3.*",
    "symfony/css-selector": "5.3.*",
    "symfony/maker-bundle": "^1.33",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/stopwatch": "5.3.*",
    "symfony/web-profiler-bundle": "5.3.*"
  }
}
